{
  "name": "quilt-benchling-webhook",
  "version": "0.4.7",
  "description": "AWS CDK deployment for Benchling webhook processing using Fargate",
  "main": "lib/benchling-webhook-stack.ts",
  "bin": {
    "benchling-webhook": "bin/benchling-webhook.ts",
    "benchling-webhook-deploy": "bin/deploy.sh"
  },
  "scripts": {
    "clean": "rm -rf cdk.out */{*.js,*.d.ts}",
    "test": "NODE_ENV=test TMPDIR=./.tmp node --max-old-space-size=4096 ./node_modules/.bin/jest",
    "cdk": "npm run test && npx cdk deploy --require-approval never",
    "deploy": "bash bin/deploy.sh",
    "deploy:skip-tests": "bash bin/deploy.sh --skip-tests",
    "lint": "eslint . --ext .ts --fix",
    "docker-sync": "node bin/docker.js sync",
    "docker-build": "node bin/docker.js build",
    "docker-push": "node bin/docker.js push",
    "docker-check": "node bin/docker.js check",
    "docker-health": "node bin/docker.js health",
    "docker-logs": "node bin/docker.js logs",
    "build": "tsc --noEmit",
    "watch": "tsc --noEmit -w",
    "synth": "npx cdk synth",
    "diff": "npx cdk diff"
  },
  "keywords": [
    "benchling",
    "webhook",
    "aws",
    "cdk",
    "fargate",
    "docker",
    "quilt"
  ],
  "author": "Quilt Data",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/quiltdata/benchling-webhook.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/adm-zip": "^0.5.7",
    "@types/aws-lambda": "^8.10.156",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.33",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "aws-cdk": "2.1031.0",
    "esbuild": "0.25.11",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "adm-zip": "^0.5.10",
    "aws-cdk-lib": "2.221.0",
    "aws-sdk": "^2.1574.0",
    "constructs": "^10.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
